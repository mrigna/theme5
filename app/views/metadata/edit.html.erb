<%= form_for(@meta, url: update_metadatum_path) do |f| %>

  <div class="field">
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </div>

  <div class="field">
    <%= f.label :description %><br>
    <%= f.text_field :description %>
  </div>

  <div class="field">
    <%= f.label :keywords %><br>
    <%= f.text_field :keywords %>
  </div>

  <div class="field"><%= f.select(:language,  ["fr", "nl", "de", "en"], :prompt => "-- Langage --") %></div>

  <div class="actions"> 
    <%= f.submit %> <%= button_to "Back", :back %>
  </div>

 

<% end %>